{
  "name": "gap-secret-editor",
  "version": "1.1.0",
  "author": "Emarsys",
  "description": "An electron-vue project",
  "repository": {
    "type": "git",
    "url": "https://github.com/emartech/gap-secret-editor.git"
  },
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "audit": "npm audit --json | audit-filter",
    "build": "node .electron-vue/build.js && electron-builder -c.mac.identity=null",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter src .electron-vue test-helpers",
    "lint:fix": "eslint --ext .js -f ./node_modules/eslint-friendly-formatter --fix src .electron-vue",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run audit && npm run lint && npm run test:once",
    "test:once": "karma start test-helpers/karma.conf.js --single-run",
    "test:watch": "karma start test-helpers/karma.conf.js",
    "postinstall": "npm run lint:fix",
    "release": "node .electron-vue/build.js && electron-builder --publish always"
  },
  "build": {
    "productName": "GAP Secret Editor",
    "appId": "com.emarsys.gap-secret-editor",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/settings.png",
      "publish": {
        "provider": "github",
        "private": true
      },
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "afterSign": "scripts/notarize.js"
  },
  "dependencies": {
    "vue": "2.6.12",
    "vue-electron": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.12.1",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.1",
    "@babel/plugin-proposal-do-expressions": "7.12.1",
    "@babel/plugin-proposal-export-default-from": "7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-function-bind": "7.12.1",
    "@babel/plugin-proposal-function-sent": "7.12.1",
    "@babel/plugin-proposal-json-strings": "7.12.1",
    "@babel/plugin-proposal-logical-assignment-operators": "7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-numeric-separator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "7.12.1",
    "@babel/plugin-proposal-throw-expressions": "7.12.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "@kubernetes/client-node": "0.12.2",
    "@vue/test-utils": "1.1.0",
    "ajv": "6.12.6",
    "audit-filter": "0.5.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "cfonts": "2.8.6",
    "chai": "4.2.0",
    "chalk": "4.1.0",
    "copy-webpack-plugin": "6.2.1",
    "cross-env": "7.0.2",
    "date-fns": "2.16.1",
    "del": "6.0.0",
    "devtron": "1.4.0",
    "electron": "10.1.3",
    "electron-builder": "22.9.1",
    "electron-debug": "3.1.0",
    "electron-devtools-installer": "3.1.1",
    "electron-notarize": "1.0.0",
    "electron-updater": "4.3.5",
    "eslint": "7.11.0",
    "eslint-config-emarsys": "5.1.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "6.1.0",
    "eslint-plugin-vue": "7.0.1",
    "file-loader": "6.1.1",
    "flush-promises": "1.0.2",
    "html-webpack-plugin": "4.5.0",
    "karma": "5.2.3",
    "karma-chai": "0.1.0",
    "karma-electron": "6.3.1",
    "karma-mocha": "2.0.1",
    "karma-sinon-chai": "2.0.2",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "4.0.2",
    "lodash": "4.17.20",
    "mocha": "8.1.3",
    "multispinner": "0.2.1",
    "node-loader": "1.0.2",
    "sinon": "9.2.0",
    "sinon-chai": "3.5.0",
    "terser-webpack-plugin": "3.0.8",
    "url-loader": "4.1.1",
    "vue-html-loader": "1.2.4",
    "vue-template-compiler": "2.6.12",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.2.0"
  }
}
